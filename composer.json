{
    "name": "compwright/oauth2-servicetitan",
    "description": "ServiceTitan OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com",
            "homepage": "https://compwright.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "servicetitan",
        "provider"
    ],
    "require": {
        "php": "^8.0",
        "league/oauth2-client": "^2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.74",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "league/oauth2-client": "^2.5",
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Compwright\\OAuth2_Servicetitan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compwright\\OAuth2_Servicetitan\\Test\\": "test/"
        }
    }
}
